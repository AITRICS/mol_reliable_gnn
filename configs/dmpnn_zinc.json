{
    "gpu": {
        "use": true,
        "id": 0
    },
    
    "model": "D_MPNN",
    "dataset": "ZINC",
    "data_seed": 222,
    "num_train": 50000,
    "num_val": 5000,
    "num_test": 5000,
    "atom_meta": true,
    
    "out_dir": "out/zinc_regression/",
    
    "params": {
        "task": "regression",
        "seed": 41,
        "epochs": 1000,
        "batch_size": 100,
        "scheduler": "plateau",
        "init_lr": 0.001,
        "lr_reduce_factor": 0.5,
        "lr_schedule_patience": 5,
        "min_lr": 1e-5,
        "weight_decay": 0.0,
        "print_epoch_interval": 5,
        "max_time": 48
    },
    
    "net_params": {
        "L": 4,
        "hidden_dim": 128,
        "out_dim": 256,
        "residual": true,
        "edge_feat": true,
        "readout": "sum",
        "in_feat_dropout": 0.0,
        "dropout": 0.0,
        "graph_norm": false,
        "layer_norm": false,
        "batch_norm": false

    }
}
